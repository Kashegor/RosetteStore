@using RosetteStore.WebUI.Models
@model RosettesListViewModel

@{
    ViewBag.Title = "Товары";
}

@foreach (var p in @Model.Rosettes)
{
    <div>
        <h3>@p.Name</h3>
        <p>@p.Description</p>
        <h4>@p.Price.ToString("# руб")</h4>
    </div>
    <div>
        @Html.PageLinks(Model.PagingInfo, x => Url.Action("List", new { page = x }))
    </div>
}